{
  "name": "FlyHUD",
  "version": "1.5.12",
  "summary": "A lightweight and easy-to-use HUD for iOS and tvOS apps.",
  "description": "FlyHUD is a lightweight and easy-to-use HUD designed to display\n the progress and status of ongoing tasks on iOS and tvOS.",
  "homepage": "https://github.com/liam-i/FlyHUD",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Liam": "liam_i@163.com"
  },
  "source": {
    "git": "https://github.com/liam-i/FlyHUD.git",
    "tag": "1.5.12"
  },
  "documentation_url": "https://liam-i.github.io/FlyHUD/main/documentation/flyhud",
  "screenshots": "https://raw.githubusercontent.com/wiki/liam-i/FlyHUD/Screenshots/1-6.png",
  "social_media_url": "https://liam-i.github.io",
  "cocoapods_version": ">= 1.13.0",
  "platforms": {
    "ios": "13.0",
    "tvos": "13.0",
    "visionos": "1.0"
  },
  "swift_versions": [
    "5.0"
  ],
  "resource_bundles": {
    "FlyHUD": [
      "Sources/HUD/PrivacyInfo.xcprivacy"
    ]
  },
  "subspecs": [
    {
      "name": "FlyHUD",
      "source_files": [
        "Sources/HUD/**/*.swift"
      ]
    },
    {
      "name": "FlyIndicatorHUD",
      "source_files": [
        "Sources/IndicatorHUD/**/*.swift"
      ],
      "dependencies": {
        "FlyHUD/FlyHUD": []
      }
    },
    {
      "name": "FlyProgressHUD",
      "source_files": [
        "Sources/ProgressHUD/**/*.swift"
      ],
      "dependencies": {
        "FlyHUD/FlyHUD": []
      }
    }
  ],
  "swift_version": "5.0"
}
